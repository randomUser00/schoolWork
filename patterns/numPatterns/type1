//5 4 3 2 1
//4 3 2 1
//3 2 1
//2 1
//1
public class pattern1
{
    public static void methodOne()
    {
        int n = 5;

        for (int i = 5; i >= 1; --i)
        {
            for (int j = i; j > 0; --j)
            {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}
